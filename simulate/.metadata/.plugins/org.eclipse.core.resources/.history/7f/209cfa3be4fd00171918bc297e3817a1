package test;

import java.util.ArrayList;

import javax.swing.JFrame;

public class CircuitTest extends JFrame {

	public static void main(String[] args)
	{
		double E = 10;
		double R1 = 1;
		double R2 = 1;
		double L1 = 1;
		double dt = 0.01;
		ArrayList<Double> Ia = new ArrayList<Double>();
		ArrayList<Double> Ib = new ArrayList<Double>();
		
		Ia.add(0.0);
		Ib.add(0.0);
		
		for(int i = 1;i <= 1000;i++)
		{
			Ia.add(((-E / R2) - ((L1 * Ib.get(i-1) / dt) / (R2 + L1 / dt))) / (- (R1 + R2) / R2) + (R2 / (R2 + L1 / dt)));
			Ib.add(((E / (R1 + R2)) + (L1 * Ib.get(i-1) / R2 / dt)) / ((- R2 / (R1 + R2)) + ((R2 + L1 / dt) / R2)));
		}
		
		for(double i : Ia)
		{
			System.out.println(i);
		}
	}
}